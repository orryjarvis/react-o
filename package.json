  },
  "homepage": "https://github.com/orryjarvis/react-o#readme",
  "engines": {
    "node": ">8.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --fix"
